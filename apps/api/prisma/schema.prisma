// Prisma schema placeholder. Full data model will arrive in PR3.
// Keeping generator/datasource so that `pnpm --filter api prisma migrate dev` works when migrations are introduced.

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
}
